{
    "name": "altelma/laravel-mailchimp",
    "description": "A packages provide you to connect with mailchimp API v3, so easiest.",
    "type": "Library",
    "keywords": [
        "mailchimp",
        "api",
        "php",
        "v3"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "ALTELMA",
            "email": "nrohtgnohp@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "Altelma\\LaravelMailChimp\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Altelma\\LaravelMailChimp\\MailChimpServiceProvider"
            ],
            "aliases": {
                "JWT": "Altelma\\LaravelMailChimp\\MailChimpFacade"
            }
        }
    }
}
